<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>AddPage</servlet-name>
        <servlet-class>database.servlets.AddCatalogServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddPage</servlet-name>
        <url-pattern>/AddPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddSubCatalog</servlet-name>
        <servlet-class>database.servlets.AddSubcatalogServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddSubCatalog</servlet-name>
        <url-pattern>/AddSubCatalog</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddFile</servlet-name>
        <servlet-class>database.servlets.AddFileServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddFile</servlet-name>
        <url-pattern>/AddFile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>show</servlet-name>
        <servlet-class>database.DBRequests</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>show</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>

</web-app>